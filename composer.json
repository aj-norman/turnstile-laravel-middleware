{
    "name": "ajnorman/turnstile-laravel-middleware",
    "description": "A package that adds middleware to check for bots using Cloudflare Turnstile",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ajnorman\\CfTurnstileLaravelMiddleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ajnorman\\CfTurnstileLaravelMiddleware\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "authors": [
        {
            "name": "Aidan Norman",
            "email": "mail@ajnorman.uk"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/http": "*",
        "illuminate/support": "^11.0",
        "nesbot/carbon": "^3.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.9",
        "pestphp/pest": "^3.7"
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "test": [
            "@clear",
            "@php vendor/bin/pest"
        ]
    }
}
